import java.util.Scanner;
public class PrintDigits{
   public static void main (String[] args)
   {String numero;
    System.out.println("dammi il numero");
    Scanner reader= new Scanner(System.in);
    numero= reader.next();
    if (numero.length() >5) 
        {System.out.println("che mi dai?");}
    else
        {int x =0;
         System.out.println("");
         while (x<5)
             {System.out.print(numero.substring(x,x+1) + " ");
              x++;
             }
         }
    }
}
